generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model Note {
  id        String    @id @default(cuid())
  views     Int       @default(0)
  title     String
  content   String
  language  String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}
